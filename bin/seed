#!/usr/bin/env node

/**
 * Module dependencies.
 */

const { run } = require("../seeders");
const logger = require("../config/logger");

/**
 * check if dropDB flag is set
 */
const dropDB = process.argv.includes("dropDB");

/**
 * Run the seeders
 */
run(dropDB)
  .then(() => {
    logger.info("Seeding Completed!");
    process.exit(0);
  })
  .catch(err => {
    logger.error(err);
    process.exit(1);
  });
